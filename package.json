{
  "name": "memologist",
  "version": "0.0.0",
  "engines": {
    "node": "16.15.1",
    "npm": "8.11.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "build": "ng build",
    "server": "http-server -p 8080 -c-1 dist/memologist/server",
    "bs": "ng build && http-server -p 8080 -c-1 dist/memologist/server",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "heroku-postbuild": "bash build.sh",
    "dev:ssr": "ng run memologist:serve-ssr",
    "serve:ssr": "node dist/memologist/server/main.js",
    "build:ssr:stage": "ng build --configuration=staging && ng run memologist:server:staging",
    "build:ssr": "ng build && ng run memologist:server",
    "prerender": "ng run memologist:prerender"
  },
